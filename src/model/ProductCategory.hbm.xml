<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	
	<class name="model.ProductCategory" table="productcategory">
	
		<id name="pcid" type="int" column="PCID">
			<generator class="increment"></generator>
		</id>
		<many-to-one name="product" class="model.Product" column="Product_Id"></many-to-one>
		<many-to-one name="category" class="model.Category" column="Category_Id"></many-to-one>
		
	</class>
</hibernate-mapping>