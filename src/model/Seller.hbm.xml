<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	
	<class name="model.Seller" table="Seller">
	
		<id name="sid" type="int" column="SID">
			<generator class="increment"></generator>
		</id>
		<property name="name">
			<column name="Name"></column>
		</property>
		
		<property name="un">
			<column name="Email"></column>
		</property>	
		
		<property name="pw">
			<column name="Password"></column>
		</property>
		
		<property name="mobile">
			<column name="Mobile"></column>
		</property>
		
		<property name="vatid">
			<column name="VATID"></column>
		</property>
		
		<property name="pan">
			<column name="PAN"></column>
		</property>
		
		<property name="accno">
			<column name="ACCNO"></column>
		</property>	
		
		<property name="ifsc">
			<column name="IFSC"></column>
		</property>
		
		<property name="address">
			<column name="Address"></column>
		</property>
		
		<property name="pincode">
			<column name="Pincode"></column>
		</property>
		
		<property name="latitude">
			<column name="Latitude"></column>
		</property>
		
		<property name="longitude">
			<column name="Longitude"></column>
		</property>
		
		<many-to-one name="city" class="model.City" column="City_ID" lazy="false"></many-to-one>
		
	</class>
</hibernate-mapping>